{
  "name": "flowpilot",
  "displayName": "FlowPilot - AI Coding Coach",
  "description": "Your AI coding coach that helps you write better code with real-time explanations and suggestions",
  "version": "0.9.3",
  "publisher": "flowpilot",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/flowpilot/flowpilot-vscode-extension.git"
  },
  "urlHandlers": [
    {
      "protocol": "flowpilot.flowpilot",
      "label": "FlowPilot Authentication"
    }
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Education",
    "Machine Learning"
  ],
  "keywords": [
    "ai",
    "coding",
    "coach",
    "education",
    "learning",
    "code explanation",
    "debugging",
    "error explanation"
  ],
  "activationEvents": [
    "onCommand:flowpilot.explainSelection",
    "onCommand:flowpilot.explainError",
    "onCommand:flowpilot.reviewFile",
    "onCommand:flowpilot.openPanel",
    "onView:flowpilotPanel"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "flowpilot",
          "title": "FlowPilot",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "flowpilot": [
        {
          "type": "webview",
          "id": "flowpilotPanel",
          "name": "FlowPilot Coach"
        }
      ]
    },
    "commands": [
      {
        "command": "flowpilot.test",
        "title": "FlowPilot: Test"
      },
      {
        "command": "flowpilot.openPanel",
        "title": "FlowPilot: Open Panel",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "flowpilot.reviewFile",
        "title": "FlowPilot: Review Entire File",
        "icon": "$(search)"
      },
      {
        "command": "flowpilot.explainSelection",
        "title": "FlowPilot: Explain Selected Code",
        "icon": "$(light-bulb)"
      },
      {
        "command": "flowpilot.explainError",
        "title": "FlowPilot: Explain Error",
        "icon": "$(bug)"
      },
      {
        "command": "flowpilot.reviewSnippet",
        "title": "FlowPilot: Review Snippet",
        "icon": "$(checklist)"
      },
      {
        "command": "flowpilot.analyzeCurrent",
        "title": "FlowPilot: Analyze Current File",
        "icon": "$(shield)"
      },
      {
        "command": "flowpilot.connect",
        "title": "FlowPilot: Connect to Dashboard"
      },
      {
        "command": "flowpilot.showWelcome",
        "title": "FlowPilot: Welcome & Walkthrough",
        "category": "FlowPilot"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "flowpilot.explainSelection",
          "when": "editorHasSelection && config.flowpilot.features.explainSelection",
          "group": "flowpilot"
        },
        {
          "command": "flowpilot.explainError",
          "when": "editorLangId && config.flowpilot.features.explainError",
          "group": "flowpilot"
        },
        {
          "command": "flowpilot.reviewSnippet",
          "when": "editorHasSelection && config.flowpilot.features.explainSelection",
          "group": "flowpilot"
        }
      ],
      "view/title": [
        {
          "command": "flowpilot.reviewFile",
          "when": "view == flowpilotPanel && config.flowpilot.features.reviewFile",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "FlowPilot",
      "properties": {
        "flowpilot.features.codeAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable automatic code analysis (linting & security)."
        },
        "flowpilot.features.explainSelection": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable 'Explain Selected Code' feature."
        },
        "flowpilot.features.explainError": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable 'Explain Error' feature."
        },
        "flowpilot.features.reviewFile": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable 'Review Entire File' feature."
        },
        "flowpilot.privacy.preferLocalModel": {
          "type": "boolean",
          "default": false,
          "description": "If enabled, use a local OpenAI-compatible endpoint for inference instead of the cloud API."
        },
        "flowpilot.local.modelEndpoint": {
          "type": "string",
          "default": "http://localhost:11434/v1/chat/completions",
          "description": "The URL for the local OpenAI-compatible inference endpoint."
        },
        "flowpilot.local.modelName": {
          "type": "string",
          "default": "llama3",
          "description": "The name of the model to use for local inference."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "bun esbuild-base -- --minify",
    "esbuild-base": "node esbuild.js",
    "compile": "tsc -p ./",
    "watch": "node esbuild.js --watch",
    "pretest": "bun compile && bun lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "version-bump": "npm version patch"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.13",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "asynckit": "^0.5.0",
    "axios": "^1.13.2",
    "call-bind-apply-helpers": "^1.0.2",
    "combined-stream": "^1.0.8",
    "delayed-stream": "^1.0.0",
    "dunder-proto": "^1.0.1",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es-object-atoms": "^1.1.1",
    "es-set-tostringtag": "^2.1.0",
    "follow-redirects": "^1.15.11",
    "form-data": "^4.0.5",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.3.0",
    "get-proto": "^1.0.1",
    "gopd": "^1.2.0",
    "has-symbols": "^1.1.0",
    "has-tostringtag": "^1.0.2",
    "hasown": "^2.0.2",
    "math-intrinsics": "^1.1.0",
    "mime-types": "^3.0.2",
    "node-fetch": "^2.7.0",
    "proxy-from-env": "^1.1.0",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-python": "^0.25.0",
    "tree-sitter-typescript": "^0.23.2",
    "web-tree-sitter": "^0.26.3"
  }
}